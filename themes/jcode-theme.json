  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#707070",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#7FD993"
      }
    },
    {
      "scope": ["string.regexp", "constant.regexp"],
      "settings": {
        "foreground": "#FFD600"
      }
    },
    {
      "scope": ["keyword", "keyword.control", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#48B9C7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#5CCFDD"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#90E0FF"
      }
    },
    {
      "scope": ["variable.parameter", "meta.parameters"],
      "settings": {
        "foreground": "#FFE066",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#5CCFDD",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#FAFAFA"
      }
    },
    {
      "scope": ["variable.language", "variable.other.constant"],
      "settings": {
        "foreground": "#D8A8F0"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#FFA876"
      }
    },
    {
      "scope": ["entity.name.tag", "support.constant"],
      "settings": {
        "foreground": "#FF5F40"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FFE066",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#48B9C7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FFE066"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D8A8F0"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#7FD993"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#5CCFDD",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#7FD993"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5F40",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#707070"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#7FD993"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5F40"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#FFD600"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#90E0FF"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#5CCFDD"
      }
    }
  ]
}
