{
  "$schema": "vscode://schemas/color-theme",
  "name": "macOS Ventura Dark - Premium Refined",
  "type": "dark",
  "author": "Inspirado no macOS Ventura - Versão Refinada Semântica",
  "semanticClass": "theme.macos.ventura.dark.premium.refined",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#5EEAD4",
    "foreground": "#E8E8EA",
    "disabledForeground": "#5A5A5F",
    "widget.shadow": "#00000060",
    "selection.background": "#5EEAD433",
    "descriptionForeground": "#A0A0A5",
    "errorForeground": "#FF6B6B",
    "icon.foreground": "#C0C0C5",
    "sash.hoverBorder": "#5EEAD4",
    "window.activeBorder": "#1A1A1C",
    "window.inactiveBorder": "#0D0D0F",
    "textBlockQuote.background": "#1A1A1C",
    "textBlockQuote.border": "#5EEAD4",
    "textCodeBlock.background": "#1A1A1C",
    "textLink.activeForeground": "#5EEAD4",
    "textLink.foreground": "#60A5FA",
    "textPreformat.foreground": "#EAB308",
    "textSeparator.foreground": "#2C2C2E",
    "button.background": "#14B8A6",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#2DD4BF",
    "button.secondaryBackground": "#2C2C2E",
    "button.secondaryForeground": "#E8E8EA",
    "button.secondaryHoverBackground": "#3A3A3C",
    "checkbox.background": "#1A1A1C",
    "checkbox.foreground": "#E8E8EA",
    "checkbox.border": "#2C2C2E",
    "dropdown.background": "#1A1A1C",
    "dropdown.border": "#2C2C2E",
    "dropdown.foreground": "#E8E8EA",
    "input.background": "#1A1A1C",
    "input.border": "#2C2C2E",
    "input.foreground": "#E8E8EA",
    "input.placeholderForeground": "#6E6E73",
    "inputOption.activeBackground": "#2C2C2E",
    "inputOption.activeBorder": "#5EEAD4",
    "inputOption.activeForeground": "#FFFFFF",
    "inputValidation.errorBackground": "#1A1A1C",
    "inputValidation.errorForeground": "#FF6B6B",
    "inputValidation.errorBorder": "#FF6B6B",
    "inputValidation.infoBackground": "#1A1A1C",
    "inputValidation.infoForeground": "#5EEAD4",
    "inputValidation.infoBorder": "#5EEAD4",
    "inputValidation.warningBackground": "#1A1A1C",
    "inputValidation.warningForeground": "#FB923C",
    "inputValidation.warningBorder": "#FB923C",
    "scrollbar.shadow": "#00000040",
    "scrollbarSlider.activeBackground": "#5EEAD480",
    "scrollbarSlider.background": "#3A3A3C40",
    "scrollbarSlider.hoverBackground": "#3A3A3C60",
    "badge.foreground": "#FFFFFF",
    "badge.background": "#14B8A6",
    "progressBar.background": "#5EEAD4",
    "list.activeSelectionBackground": "#2C2C2E",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.focusBackground": "#2C2C2E",
    "list.focusForeground": "#FFFFFF",
    "list.focusOutline": "#5EEAD4",
    "list.highlightForeground": "#5EEAD4",
    "list.hoverBackground": "#1F1F21",
    "list.inactiveSelectionBackground": "#252527",
    "list.errorForeground": "#FF6B6B",
    "list.warningForeground": "#FB923C",
    "activityBar.background": "#0A0A0B",
    "activityBar.foreground": "#E8E8EA",
    "activityBar.inactiveForeground": "#6E6E73",
    "activityBar.border": "#1A1A1C",
    "activityBarBadge.background": "#14B8A6",
    "activityBarBadge.foreground": "#FFFFFF",
    "activityBar.activeBorder": "#5EEAD4",
    "sideBar.background": "#121214",
    "sideBar.foreground": "#C0C0C5",
    "sideBar.border": "#1A1A1C",
    "sideBarTitle.foreground": "#E8E8EA",
    "sideBarSectionHeader.background": "#0A0A0B",
    "sideBarSectionHeader.foreground": "#E8E8EA",
    "editorGroup.border": "#1A1A1C",
    "editorGroupHeader.tabsBackground": "#0A0A0B",
    "editorGroupHeader.tabsBorder": "#1A1A1C",
    "tab.activeBackground": "#0D0D0F",
    "tab.activeForeground": "#E8E8EA",
    "tab.activeBorderTop": "#5EEAD4",
    "tab.inactiveBackground": "#0A0A0B",
    "tab.inactiveForeground": "#6E6E73",
    "tab.hoverBackground": "#1A1A1C",
    "tab.hoverBorder": "#5EEAD480",
    "editor.background": "#0D0D0F",
    "editor.foreground": "#E8E8EA",
    "editorLineNumber.foreground": "#3A3A3C",
    "editorLineNumber.activeForeground": "#6E6E73",
    "editorCursor.foreground": "#5EEAD4",
    "editor.selectionBackground": "#5EEAD428",
    "editor.inactiveSelectionBackground": "#5EEAD415",
    "editor.selectionHighlightBackground": "#60A5FA20",
    "editor.wordHighlightBackground": "#A78BFA20",
    "editor.findMatchBackground": "#5EEAD450",
    "editor.findMatchHighlightBackground": "#5EEAD430",
    "editor.lineHighlightBackground": "#1A1A1C",
    "editorBracketMatch.background": "#5EEAD430",
    "editorBracketMatch.border": "#5EEAD4",
    "editorBracketHighlight.foreground1": "#5EEAD4",
    "editorBracketHighlight.foreground2": "#60A5FA",
    "editorBracketHighlight.foreground3": "#A78BFA",
    "editorBracketHighlight.foreground4": "#EAB308",
    "editorBracketHighlight.foreground5": "#FB923C",
    "editorBracketHighlight.foreground6": "#2DD4BF",
    "editorError.foreground": "#FF6B6B",
    "editorWarning.foreground": "#FB923C",
    "editorInfo.foreground": "#5EEAD4",
    "editorGutter.modifiedBackground": "#60A5FA",
    "editorGutter.addedBackground": "#2DD4BF",
    "editorGutter.deletedBackground": "#FF6B6B",
    "statusBar.background": "#0A0A0B",
    "statusBar.foreground": "#A0A0A5",
    "statusBar.border": "#1A1A1C",
    "statusBar.debuggingBackground": "#14B8A6",
    "statusBarItem.errorBackground": "#FF6B6B",
    "statusBarItem.warningBackground": "#FB923C",
    "titleBar.activeBackground": "#0A0A0B",
    "titleBar.activeForeground": "#E8E8EA",
    "titleBar.inactiveBackground": "#0D0D0F",
    "titleBar.inactiveForeground": "#6E6E73",
    "terminal.background": "#0D0D0F",
    "terminal.foreground": "#E8E8EA",
    "terminal.selectionBackground": "#5EEAD433",
    "terminalCursor.foreground": "#5EEAD4",
    "charts.red": "#FF6B6B",
    "charts.blue": "#60A5FA",
    "charts.yellow": "#EAB308",
    "charts.orange": "#FB923C",
    "charts.green": "#2DD4BF",
    "charts.purple": "#A78BFA"
  },
  "semanticTokenColors": {
    // Mapeamento semântico universal, priorizado para clareza e consistência entre linguagens
    // Tipos / Interfaces / Structs / Enums / Anotações de tipo (ex: TS types, Rust structs, Java interfaces, C# enums)
    "type": {
      "foreground": "#5EEAD4"
    },
    "interface": {
      "foreground": "#5EEAD4"
    },
    "struct": {
      "foreground": "#5EEAD4"
    },
    "enum": {
      "foreground": "#5EEAD4"
    },
    "typeParameter": {
      "foreground": "#5EEAD4",
      "fontStyle": "italic"
    },
    // Classes / Traits / Protocols / Tipos abstratos (ex: Java classes, Rust traits, Go interfaces, C# abstract classes)
    "class": {
      "foreground": "#2DD4BF"  // Variação mais escura de teal para distinção
    },
    // Funções / Métodos / Procedimentos (ex: JS functions, Python def, Java methods, Rust fn, C++ functions)
    "function": {
      "foreground": "#60A5FA"
    },
    "method": {
      "foreground": "#60A5FA"
    },
    // Keywords de controle e fluxo (ex: if, for, async, return, match, try, catch em várias linguagens)
    "keyword": {
      "foreground": "#A78BFA",
      "fontStyle": "bold"
    },
    // Strings (ex: em todas as linguagens, incluindo templates e regex)
    "string": {
      "foreground": "#EAB308"
    },
    // Números / Literais / Constantes (ex: numbers, booleans, null em JS/TS, const em Rust/C++)
    "number": {
      "foreground": "#FB923C"
    },
    "enumMember": {
      "foreground": "#FB923C"
    },
    "macro": {
      "foreground": "#FB923C"
    },
    // Variáveis locais (ex: let/var em JS, locais em Python/Java)
    "variable": {
      "foreground": "#E8E8EA"
    },
    // Parâmetros / Argumentos (ex: function params em todas as linguagens)
    "parameter": {
      "foreground": "#C0C0C5",
      "fontStyle": "italic"
    },
    // Namespaces / Packages / Modules / Imports (ex: import in Python/JS, package in Java/Go, namespace in C++/C#)
    "namespace": {
      "foreground": "#A0A0A5"
    },
    "module": {
      "foreground": "#A0A0A5"
    },
    // Comentários / Docstrings (ex: //, /* */, #, /// em várias linguagens)
    "comment": {
      "foreground": "#6B7280",
      "fontStyle": "italic"
    },
    // Operadores e pontuação para suporte geral
    "operator": {
      "foreground": "#A0A0A5"
    },
    "property": {
      "foreground": "#E8E8EA"
    },
    "regexp": {
      "foreground": "#FB923C"
    }
  },
  "tokenColors": [
    // TokenColors como fallback para linguagens sem suporte semântico pleno ou scopes específicos
    // Comentários
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#6B7280",
        "fontStyle": "italic"
      }
    },
    // Strings
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template", "punctuation.definition.string"],
      "settings": {
        "foreground": "#EAB308"
      }
    },
    {
      "name": "String Interpolation / Regexp",
      "scope": ["string.interpolated", "string.regexp", "punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#EAB308"
      }
    },
    // Números / Constantes
    {
      "name": "Number / Constant / Literal",
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#FB923C"
      }
    },
    // Variáveis / Propriedades
    {
      "name": "Variable / Property",
      "scope": ["variable", "variable.other", "meta.object-literal.key", "variable.object.property", "support.variable", "entity.other.attribute-name", "support.type.property-name"],
      "settings": {
        "foreground": "#E8E8EA"
      }
    },
    // Parâmetros
    {
      "name": "Parameter",
      "scope": ["variable.parameter", "meta.function.parameters"],
      "settings": {
        "foreground": "#C0C0C5",
        "fontStyle": "italic"
      }
    },
    // Keywords / Storage
    {
      "name": "Keyword / Control / Storage",
      "scope": ["keyword", "keyword.control", "keyword.operator.new", "keyword.other", "storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#A78BFA",
        "fontStyle": "bold"
      }
    },
    // Operadores
    {
      "name": "Operator",
      "scope": ["keyword.operator", "keyword.operator.logical", "keyword.operator.arithmetic", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#A0A0A5"
      }
    },
    // Funções / Métodos
    {
      "name": "Function / Method",
      "scope": ["entity.name.function", "meta.function-call", "support.function", "entity.name.function.python", "entity.name.function.js", "entity.name.function.ts"],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    // Classes / Tipos
    {
      "name": "Class / Type",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class", "entity.name.type", "support.type", "entity.name.type.ts", "entity.name.type.interface.ts"],
      "settings": {
        "foreground": "#5EEAD4"
      }
    },
    // Herança / Interfaces
    {
      "name": "Inherited Class / Interface",
      "scope": ["entity.other.inherited-class", "entity.name.type.interface"],
      "settings": {
        "foreground": "#2DD4BF"
      }
    },
    // Namespaces / Módulos / Imports
    {
      "name": "Namespace / Module / Import",
      "scope": ["entity.name.namespace", "support.module", "keyword.control.import", "keyword.control.export", "keyword.control.from", "meta.import", "meta.export"],
      "settings": {
        "foreground": "#A0A0A5"
      }
    },
    // Decorators
    {
      "name": "Decorator / Annotation",
      "scope": ["meta.decorator", "punctuation.decorator", "entity.name.function.decorator.python"],
      "settings": {
        "foreground": "#A78BFA"
      }
    },
    // Tags / Atributos (para HTML/JSX)
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#5EEAD4"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": ["entity.other.attribute-name.html", "entity.other.attribute-name.jsx"],
      "settings": {
        "foreground": "#E8E8EA"
      }
    },
    // Componentes JSX
    {
      "name": "JSX Component",
      "scope": ["support.class.component.jsx", "support.class.component.tsx"],
      "settings": {
        "foreground": "#5EEAD4"
      }
    },
    // CSS Específicos
    {
      "name": "CSS Property Name",
      "scope": ["support.type.property-name.css", "meta.property-name.css"],
      "settings": {
        "foreground": "#E8E8EA"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": ["support.constant.property-value.css", "meta.property-value.css"],
      "settings": {
        "foreground": "#FB923C"
      }
    },
    {
      "name": "CSS Class / ID",
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    // Markdown
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#5EEAD4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code", "markup.raw"],
      "settings": {
        "foreground": "#EAB308"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link", "string.other.link.title.markdown"],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    // Inválidos
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#FF6B6B",
        "fontStyle": "underline"
      }
    },
    // Diff
    {
      "name": "Diff Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#2DD4BF"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Diff Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    // Pontuação Geral
    {
      "name": "Punctuation",
      "scope": ["punctuation", "punctuation.separator", "punctuation.terminator", "punctuation.definition.tag", "meta.brace", "punctuation.section", "punctuation.section.embedded"],
      "settings": {
        "foreground": "#A0A0A5"
      }
    }
  ]
}
