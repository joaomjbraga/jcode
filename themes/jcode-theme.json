  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#70A060",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "punctuation.definition.string"],
      "settings": {
        "foreground": "#E0A080"
      }
    },
    {
      "scope": ["string.regexp", "constant.regexp"],
      "settings": {
        "foreground": "#D09090"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#B0E0B0"
      }
    },
    {
      "scope": ["keyword", "keyword.control", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#80C0FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#E0E0E0"
      }
    },
    {
      "scope": ["punctuation", "punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#FFFFB0"
      }
    },
    {
      "scope": ["variable.parameter", "meta.parameters"],
      "settings": {
        "foreground": "#B0E0FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#80FFD0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.namespace"],
      "settings": {
        "foreground": "#80C0FF"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.other.object"],
      "settings": {
        "foreground": "#E8E8E8"
      }
    },
    {
      "scope": ["variable.language.this", "variable.language.super", "variable.other.constant"],
      "settings": {
        "foreground": "#FFB0FF"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#B0E0FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#80FFD0"
      }
    },
    {
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#B0E0FF"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#80C0FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FFFFB0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#FFB0FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#80C0FF",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#E0A080"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF8080",
        "fontStyle": "bold underline"
      }
    },
    {
      "scope": ["meta.diff.header", "meta.diff.range"],
      "settings": {
        "foreground": "#80C0FF"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#80FF80"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF8080"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#FFFF80"
      }
    }
  ]
}
