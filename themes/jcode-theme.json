{
  "$schema": "vscode://schemas/color-theme",
  "name": "jcode",
  "type": "dark",
  "author": "Jo√£o M J Braga",
  "semanticHighlighting": true,
  "colors": {
    "foreground": "#E1E1E6",
    "descriptionForeground": "#B0B8C8",
    "focusBorder": "#FF92D080",
    "contrastBorder": "#000000",
    "errorForeground": "#FF7088",

    "editor.background": "#141414",
    "editor.foreground": "#E1E1E6",
    "editorLineNumber.foreground": "#606070",
    "editorLineNumber.activeForeground": "#9A9AB0",
    "editorCursor.foreground": "#79FF97",
    "editor.selectionBackground": "#41414DEE",
    "editor.selectionHighlightBackground": "#B28CFF44",
    "editor.inactiveSelectionBackground": "#41414DAA",
    "editor.wordHighlightBackground": "#B28CFF66",
    "editor.wordHighlightStrongBackground": "#79FF9777",
    "editor.findMatchBackground": "#FFFFA8EE",
    "editor.findMatchHighlightBackground": "#FFFFA899",
    "editor.lineHighlightBackground": "#1E1E1ECC",
    "editor.lineHighlightBorder": "#2A2A2A",
    "editor.rangeHighlightBackground": "#78D1E133",
    "editor.hoverHighlightBackground": "#41414D88",
    "editorIndentGuide.background": "#FFFFFF18",
    "editorIndentGuide.activeBackground": "#E1E1E666",
    "editorBracketMatch.background": "#79FF9755",
    "editorBracketMatch.border": "#79FF97AA",
    "editorBracketHighlight.foreground1": "#79FF97",
    "editorBracketHighlight.foreground2": "#6EE1FF",
    "editorBracketHighlight.foreground3": "#B28CFF",
    "editorBracketHighlight.foreground4": "#FFFFA8",
    "editorBracketHighlight.foreground5": "#FFB884",
    "editorBracketHighlight.foreground6": "#FF92D0",
    "editorBracketHighlight.unexpectedBracket.foreground": "#FF7088",
    "editorError.foreground": "#FF7088",
    "editorWarning.foreground": "#FFB884",
    "editorInfo.foreground": "#6EE1FF",
    "editorHint.foreground": "#79FF97",
    "editorGutter.modifiedBackground": "#B28CFFBB",
    "editorGutter.addedBackground": "#79FF97BB",
    "editorGutter.deletedBackground": "#FF7088BB",

    "activityBar.background": "#141414",
    "activityBar.foreground": "#E1E1E6",
    "activityBar.inactiveForeground": "#606070",
    "activityBar.activeBorder": "#FF92D0BB",
    "activityBar.activeBackground": "#1E1E1EBB",
    "activityBarBadge.background": "#FF92D0",
    "activityBarBadge.foreground": "#141414",

    "sideBar.background": "#141414",
    "sideBar.foreground": "#E1E1E6",
    "sideBarTitle.foreground": "#E1E1E6",
    "sideBarSectionHeader.background": "#1A1A1A",
    "sideBarSectionHeader.foreground": "#E1E1E6",

    "statusBar.background": "#0F0F0F",
    "statusBar.foreground": "#E1E1E6",
    "statusBar.debuggingBackground": "#FF7088",
    "statusBar.noFolderBackground": "#0F0F0F",

    "titleBar.activeBackground": "#141414",
    "titleBar.activeForeground": "#E1E1E6",
    "titleBar.inactiveBackground": "#1A1A1A",
    "titleBar.inactiveForeground": "#88889A",

    "tab.activeBackground": "#141414",
    "tab.activeForeground": "#E1E1E6",
    "tab.inactiveBackground": "#1A1A1A",
    "tab.inactiveForeground": "#88889A",
    "tab.activeBorderTop": "#FF92D0CC",
    "tab.hoverBackground": "#1E1E1ECC",

    "panel.background": "#141414",
    "panel.border": "#2A2A2A",
    "panelTitle.activeBorder": "#FF92D0",
    "panelTitle.activeForeground": "#E1E1E6",

    "terminal.background": "#141414",
    "terminal.foreground": "#E1E1E6",
    "terminalCursor.foreground": "#79FF97",
    "terminal.selectionBackground": "#41414DBB",
    "terminal.ansiBlack": "#1E1E1E",
    "terminal.ansiRed": "#FF92D0",
    "terminal.ansiGreen": "#79FF97",
    "terminal.ansiYellow": "#FFFFA8",
    "terminal.ansiBlue": "#6EE1FF",
    "terminal.ansiMagenta": "#B28CFF",
    "terminal.ansiCyan": "#A1EFE4",
    "terminal.ansiWhite": "#E1E1E6",
    "terminal.ansiBrightRed": "#FF7088",
    "terminal.ansiBrightGreen": "#79FF97",
    "terminal.ansiBrightYellow": "#FFFFA8",
    "terminal.ansiBrightBlue": "#6EE1FF",
    "terminal.ansiBrightMagenta": "#B28CFF",
    "terminal.ansiBrightCyan": "#A4FFFF",
    "terminal.ansiBrightWhite": "#F7F7FB"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#78829A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage", "keyword.control", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF92D0"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#79FF97"
      }
    },
    {
      "name": "Types & Classes",
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#B28CFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#FFFFA8"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": ["constant.numeric", "constant.language", "constant.other"],
      "settings": {
        "foreground": "#FFB884"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FFB884",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": ["variable", "variable.other", "support.variable", "meta.object-literal.key"],
      "settings": {
        "foreground": "#E1E1E6"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF92D0"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#FF92D0"
      }
    },
    {
      "name": "Escape Characters & Regex",
      "scope": ["constant.character.escape", "string.regexp"],
      "settings": {
        "foreground": "#FF92D0"
      }
    },
    {
      "name": "Built-in / Support",
      "scope": ["support.function", "support.type", "support.class"],
      "settings": {
        "foreground": "#6EE1FF"
      }
    },
    {
      "name": "This / Self / Super",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#6EE1FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF7088",
        "fontStyle": "underline"
      }
    }
  ]
}
