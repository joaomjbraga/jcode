  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A8A8C0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#B0FF50"
      }
    },
    {
      "scope": ["constant", "variable.other.constant", "variable.language"],
      "settings": {
        "foreground": "#70A8FF"
      }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#FF7070"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#FFF0C0"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#C090FF"
      }
    },
    {
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": {
        "foreground": "#A080FF"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#C090FF"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#C090FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FFF0C0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#70A8FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF7070",
        "fontStyle": "underline italic"
      }
    },
    {
      "scope": ["punctuation.definition.string.begin", "punctuation.definition.string.end"],
      "settings": {
        "foreground": "#FFFFB0"
      }
    }
  ]
}